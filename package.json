{
  "name": "@increase/components",
  "version": "0.0.1",
  "description": "React component library for Increase design system",
  "sideEffects": false,
  "main": "index.js",
  "module": "index.js",
  "files": [
    "dist",
    "README.md",
    "index.js"
  ],
  "scripts": {
    "start": "npm run storybook",
    "prepublishOnly": "npm run build",
    "build": "NODE_ENV=production rollup -c",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs",
    "lint": "eslint \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IncreaseCard/increase-components.git"
  },
  "author": "Santiago Ocamica <santiago.ocamica@increasecard.com>",
  "bugs": {
    "url": "https://github.com/IncreaseCard/increase-components/issues"
  },
  "homepage": "https://github.com/IncreaseCard/increase-components#readme",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@storybook/addon-a11y": "^5.0.11",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addon-viewport": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/react": "^16.8.17",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.0.0",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "23.6.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "normalize.css": "^8.0.1",
    "prettier": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.4",
    "reactjs-popup": "^1.4.0",
    "rollup": "^1.11.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-url": "^2.2.1",
    "styled-components": "^4.2.0"
  },
  "peerDependencies": {
    "styled-components": "^4.0",
    "react": ">=16.7",
    "react-dom": ">=16.7",
    "reactjs-popup": "^1.3.2",
    "prop-types": "^15.6",
    "moment": "^2.24",
    "moment-range": "^4.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
