{
  "name": "increase-components",
  "version": "6.0.0",
  "description": "React component library for Increase design system",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "index.js"
  ],
  "scripts": {
    "start": "npm run storybook",
    "prepublishOnly": "npm run build",
    "build": "webpack -p --progress --colors --config webpack.config.publish.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IncreaseCard/increase-components.git"
  },
  "author": "Santiago Ocamica <santiago.ocamica@increasecard.com>",
  "bugs": {
    "url": "https://github.com/IncreaseCard/increase-components/issues"
  },
  "homepage": "https://github.com/IncreaseCard/increase-components#readme",
  "license": "UNLICENSED",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@storybook/addon-a11y": "^4.1.1",
    "@storybook/addon-actions": "^4.1.1",
    "@storybook/addon-info": "^4.1.1",
    "@storybook/addon-links": "^4.1.1",
    "@storybook/addon-viewport": "^4.1.1",
    "@storybook/addons": "^4.1.1",
    "@storybook/react": "^4.1.1",
    "@types/react": "^16.7.17",
    "babel-loader": "^8.0.4",
    "babel-preset-react-app": "^6.1.0",
    "eslint": "5.10.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "23.6.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.23.0",
    "moment-range": "^4.0.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.2",
    "terser-webpack-plugin": "^1.1.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "peerDependencies": {
    "styled-components": "^4.0",
    "react": ">=16.3",
    "react-dom": ">=16.3",
    "prop-types": "^15.6",
    "moment": "^2.22",
    "moment-range": "^3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
